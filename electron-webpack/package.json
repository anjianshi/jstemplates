{
  "private": true,
  "name": "{TMP:your-app-name}",
  "version": "0.0.1",
  "main": "main/index.js",
  "scripts": {
    "compile": "yarn run clear && yarn run compile-main && yarn run compile-renderer && yarn run dist-install-deps",
    "compile-hmr": "yarn run clear && yarn run compile-main-hmr && yarn run dist-install-deps && yarn run compile-renderer-hmr",
    "start": "electron var/app_dist/main/index.js",
    "compile-and-start": "yarn run compile && yarn run start",
    "package": "PLATFORM=linux yarn run raw-package",
    "package-win": "PLATFORM=win32 yarn run raw-package",
    "clear": "rimraf var/app_dist",
    "compile-main": "webpack --config build/main.webpack.js",
    "compile-renderer": "webpack --config build/renderer.webpack.js",
    "compile-main-hmr": "NODE_ENV=development HMR=1 yarn run compile-main",
    "compile-renderer-hmr": "NODE_ENV=development HMR=1 node build/renderer_hmr.js",
    "dist-install-deps": "cd var/app_dist && yarn install",
    "raw-package": "NODE_ENV=production yarn run compile && NODE_ENV=production electron-packager var/app_dist --platform=$PLATFORM --arch=x64 --overwrite --out var"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-anjianshi-react": "^0.0.4",
    "css-loader": "^0.25.0",
    "devtron": "^1.4.0",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.4.3",
    "eslint": "^3.8.0",
    "eslint-config-anjianshi-react": "^0.0.5",
    "express": "^4.13.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.16.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
